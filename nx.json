{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["compile", "build"]
      }
    }
  },
  "targetDefaults": {
    "start": {
      "dependsOn": ["^compile"]
    },
    "build": {
      "dependsOn": ["^compile"]
    },
    "compile": {
      "dependsOn": ["^compile"]
    },
    "clean": {
      "dependsOn": ["^clean"]
    }
  }
}
